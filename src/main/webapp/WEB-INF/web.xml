	</servlet>

	<servlet>
		<servlet-name>adminSubmissionDetail</servlet-name>
		<servlet-class>onlineQuiz.controller.adminSubdetaillServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>adminProfile</servlet-name>
		<servlet-class>onlineQuiz.controller.adminProfileServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>adminModified</servlet-name>
		<servlet-class>onlineQuiz.controller.adminModifyServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>adminQuiz</servlet-name>
		<servlet-class>onlineQuiz.controller.adminQuizServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>adminAddQuestion</servlet-name>
		<servlet-class>onlineQuiz.controller.adminAddServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>adminEditQuestion</servlet-name>
		<servlet-class>onlineQuiz.controller.adminEditServlet</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>register</servlet-name>
		<url-pattern>/register</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>logout</servlet-name>
		<url-pattern>/logout</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>quiz</servlet-name>
		<url-pattern>/page/quiz</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>question</servlet-name>
		<url-pattern>/page/question</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>submission</servlet-name>
		<url-pattern>/page/submission</url-pattern>
	</servlet-mapping>
  
  <servlet-mapping>
    <servlet-name>feedback</servlet-name>
    <url-pattern>/page/feedback</url-pattern>
  </servlet-mapping>
  
