<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://xmlns.jcp.org/xml/ns/javaee"
	xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
	id="assignment1" version="3.1">

	<display-name>Online Assessment</display-name>

	<welcome-file-list>
		<welcome-file>login</welcome-file>
	</welcome-file-list>

	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>onlineQuiz.controller.LoginServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>register</servlet-name>
		<servlet-class>onlineQuiz.controller.RegisterServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>logout</servlet-name>
		<servlet-class>onlineQuiz.controller.LogoutServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>quiz</servlet-name>
		<servlet-class>onlineQuiz.controller.QuizServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>question</servlet-name>
		<servlet-class>onlineQuiz.controller.QuestionServlet</servlet-class>
	</servlet>
	
	<servlet>
		<servlet-name>submission</servlet-name>
		<servlet-class>onlineQuiz.controller.SubmissionServlet</servlet-class>
	</servlet>
  
  <servlet>
    <servlet-name>feedback</servlet-name>
    <servlet-class>onlineQuiz.controller.FeedbackServlet</servlet-class>
  </servlet>
  
  <servlet>
    <servlet-name>contact</servlet-name>
    <servlet-class>onlineQuiz.controller.ContactServlet</servlet-class>
  </servlet>

	<servlet>
		<servlet-name>userpage</servlet-name>
		<servlet-class>onlineQuiz.controller.UserPageServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>admin</servlet-name>
		<servlet-class>onlineQuiz.controller.adminServlet</servlet-class>
	</servlet>

	<servlet>
		<servlet-name>adminSubmission</servlet-name>
